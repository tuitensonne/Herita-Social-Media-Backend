generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  ID        Int    @id @default(autoincrement())
  email     String @unique @db.VarChar(100)
  password  String @db.VarChar(100)
  username  String @db.VarChar(100)
}
